{
	"name": "co-simulation-studio",
	"description": "Co-Simulation Studio",
	"version": "0.0.0",
	"private": true,
	"engines": {
		"vscode": "^1.77.0"
	},
	"categories": [],
	"activationEvents": [
		"*"
	],
	"browser": "./extension/dist/browserMain",
	"contributes": {
		"configuration": {
			"title": "Co-Simulation Studio",
			"properties": {
				"cosimulationstudio.developmentMode": {
					"title": "Development Mode",
					"type": "boolean",
					"default": false
				},
				"cosimulationstudio.activeProjectLocation": {
					"title": "Active Project Location",
					"type": "string",
					"default": "${workspaceFolder}"
				},
				"cosimulationstudio.downloadsInstallationLocation": {
					"title": "Downloads Installation Location",
					"type": "string",
					"default": "${userHome}/into-cps-projects/install"
				},
				"cosimulationstudio.downloadsLocation": {
					"title": "Downloads Location",
					"type": "string",
					"default": "${userHome}/into-cps-projects/install_downloads"
				},
				"cosimulationstudio.coeUrl": {
					"title": "COE URL",
					"type": "string",
					"default": "localhost:8082"
				},
				"cosimulationstudio.traceabilityDaemonPort": {
					"title": "Traceability Daemon Port",
					"type": "string",
					"default": "8083"
				},
				"cosimulationstudio.coeIsRemote": {
					"title": "COE Is Remote",
					"type": "boolean",
					"default": false
				},
				"cosimulationstudio.coeJarLocation": {
					"title": "COE .jar Location",
					"type": "string",
					"default": "${userHome}/into-cps-projects/install_downloads"
				},
				"cosimulationstudio.rtTesterInstallationLocation": {
					"title": "RT-Tester Installation Location",
					"type": [
						"string",
						"null"
					],
					"default": null
				},
				"cosimulationstudio.rtTesterMbtInstallationLocation": {
					"title": "RT-Tester MBT Installation Location",
					"type": [
						"string",
						"null"
					],
					"default": null
				},
				"cosimulationstudio.rtTesterRttui3ExecutableLocation": {
					"title": "RT-Tester RTTUI3 Executable Location",
					"type": [
						"string",
						"null"
					],
					"default": null
				},
				"cosimulationstudio.pythonExecutableForRtTester": {
					"title": "Python Executable for RT-Tester",
					"type": [
						"string",
						"null"
					],
					"default": null
				},
				"cosimulationstudio.updateSite": {
					"title": "Update Site",
					"type": "string",
					"default": "https://raw.githubusercontent.com/INTO-CPS-Association/INTO-CPS-Association.github.io/master/download/"
				},
				"cosimulationstudio.developmentModeUpdateSite": {
					"title": "Development Mode Update Site",
					"type": "string",
					"default": "https://raw.githubusercontent.com/INTO-CPS-Association/INTO-CPS-Association.github.io/development/download/"
				},
				"cosimulationstudio.examplesUrl": {
					"title": "Examples URL",
					"type": "string",
					"default": "https://raw.githubusercontent.com/INTO-CPS-Association/INTO-CPS-Association.github.io/master/examples/examples.json"
				},
				"cosimulationstudio.developmentModeExamplesUrl": {
					"title": "Development Mode Examples URL",
					"type": "string",
					"default": "https://raw.githubusercontent.com/INTO-CPS-Association/INTO-CPS-Association.github.io/development/examples/examples.json"
				},
				"cosimulationstudio.defaultProjectLocation": {
					"title": "Default Project Location",
					"type": "string",
					"default": "${userHome}/into-cps-projects"
				},
				"cosimulationstudio.enableTraceability": {
					"title": "Enable Traceability",
					"type": "boolean",
					"default": false
				},
				"cosimulationstudio.localUpdateSite": {
					"title": "Local Update Site",
					"type": [
						"string",
						"null"
					],
					"default": null
				},
				"cosimulationstudio.enableLocalUpdateSite": {
					"title": "Enable Local Update Site",
					"type": "boolean",
					"default": false
				},
				"cosimulationstudio.numberOfSamplesInGraph": {
					"title": "Number of Samples in Graph",
					"type": "integer",
					"default": 1000
				}
			}
		},
		"customEditors": [
			{
				"viewType": "cosimulationstudio.editors.appEditor",
				"displayName": "App Editor",
				"selector": [
					{
						"filenamePattern": "*.json"
					}
				],
				"priority": "default"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "co-simulation-studio-workspace",
					"icon": "$(repo)",
					"title": "Co-Simulation Studio Workspace"
				}
			]
		},
		"views": {
			"co-simulation-studio-workspace": [
				{
					"id": "dseView",
					"name": "DSES",
					"icon": "media/dep.svg",
					"contextualTitle": "DSE View",
					"visibility": "collapsed"
				},
				{
					"id": "fmuView",
					"name": "FMUS",
					"icon": "media/dep.svg",
					"contextualTitle": "FMU View",
					"visibility": "visible",
					"initialSize": 500
				},
				{
					"id": "modelcheckView",
					"name": "Model Checking",
					"icon": "media/dep.svg",
					"contextualTitle": "Model Checking View",
					"visibility": "collapsed"
				},
				{
					"id": "modelView",
					"name": "Models",
					"icon": "media/dep.svg",
					"contextualTitle": "Models View",
					"visibility": "visible"
				},
				{
					"id": "multimodelView",
					"name": "Multi-models",
					"icon": "media/dep.svg",
					"contextualTitle": "Multi Models View",
					"visibility": "visible"
				},
				{
					"id": "resourcesView",
					"name": "Resources",
					"icon": "media/dep.svg",
					"contextualTitle": "Resources View",
					"visibility": "collapsed"
				},
				{
					"id": "sigverView",
					"name": "Sigver",
					"icon": "media/dep.svg",
					"contextualTitle": "Sigver View",
					"visibility": "collapsed"
				},
				{
					"id": "sysmlView",
					"name": "SysML",
					"icon": "media/dep.svg",
					"contextualTitle": "SysML View",
					"visibility": "collapsed"
				},
				{
					"id": "TestDataGenView",
					"name": "Test Data Geneneration",
					"icon": "media/dep.svg",
					"contextualTitle": "Test data generation View",
					"visibility": "collapsed"
				},
				{
					"id": "traceabilityView",
					"name": "Traceability",
					"icon": "media/dep.svg",
					"contextualTitle": "Traceability View",
					"visibility": "collapsed"
				},
				{
					"id": "usermetricscriptsView",
					"name": "User Metric Scripts",
					"icon": "media/dep.svg",
					"contextualTitle": "User Metric Scripts View",
					"visibility": "collapsed"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "cd components && npm run build && cd ../webviews && npm run build && cd .. && webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint ./extension/src ./language-server/src --ext .ts,.tsx",
		"postinstall": "cd components && npm install && cd ../webviews && npm install && cd ../extension && npm install && cd ../language-server && npm install && cd ..",
		"chrome": "vscode-test-web --host=0.0.0.0 --quality=insiders --extensionDevelopmentPath=. --extensionId=vscode-icons-team.vscode-icons"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"@vscode/test-web": "^0.0.22",
		"css-loader": "^6.7.3",
		"eslint": "^8.26.0",
		"path-browserify": "^1.0.1",
		"style-loader": "^3.3.2",
		"ts-loader": "^9.3.0",
		"typescript": "^5.0.2",
		"webpack": "^5.44.0",
		"webpack-cli": "^4.7.2"
	},
	"dependencies": {
		"process": "^0.11.10"
	}
}